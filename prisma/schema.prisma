generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Bets {
  value      Float?    @db.Real
  created_at DateTime? @default(now()) @db.Timestamp(6)
  id         String    @id(map: "bets_pk") @default(uuid()) @db.Uuid

  @@map("bets")
}
